{
	"name": "cf-rpc",
	"version": "0.0.20",
	"private": false,
	"scripts": {
		"deploy": "wrangler deploy",
		"prepublishOnly": "npm run build",
		"dev": "wrangler dev",
		"build": "tsup && node fixBuildTypes.js",
		"start": "wrangler dev",
		"test": "vitest --ui"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"module": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./cors": {
			"types": "./dist/cors.d.ts",
			"import": "./dist/cors.mjs",
			"module": "./dist/cors.mjs",
			"require": "./dist/cors.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"module": "./dist/client.mjs",
			"require": "./dist/client.js",
			"svelte": "./dist/client.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.1.19",
		"@cloudflare/workers-types": "^4.20240512.0",
		"@types/cookie": "^0.6.0",
		"@types/js-cookie": "^3.0.6",
		"@vitest/ui": "^1.6.0",
		"estrella": "^1.4.1",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"valibot": "^0.30.0",
		"vitest": "1.3.0",
		"wrangler": "^3.57.1",
		"zod": "^3.23.8"
	},
	"dependencies": {
		"ampliform": "^0.0.9",
		"cookie": "^0.6.0"
	}
}
